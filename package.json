{
  "name": "qr-attendance-system",
  "version": "1.0.0",
  "description": "Web-based student attendance system using dynamic QR codes",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node --watch server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "db:create": "node scripts/create-db.js",
    "db:init": "node scripts/init-db.js",
    "db:setup": "node scripts/create-db.js && node scripts/init-db.js",
    "setup:courses": "node setup-courses.js",
    "create:admin": "node scripts/create_admin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tanya9270/NIELIT_Attendance.git"
  },
  "bugs": {
    "url": "https://github.com/Tanya9270/NIELIT_Attendance/issues"
  },
  "homepage": "https://github.com/Tanya9270/NIELIT_Attendance#readme",
  "keywords": [
    "attendance",
    "qr-code",
    "react",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.17.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "concurrently": "^8.2.2",
    "typescript": "^5.9.3"
  }
}
